{
  "name": "pay-selfservice",
  "description": "Payments Selfservice application",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^22.16.0"
  },
  "_moduleAliases": {
    "@root": "src",
    "@controllers": "src/controllers",
    "@middleware": "src/middleware",
    "@models": "src/models",
    "@services": "src/services",
    "@utils": "src/utils",
    "@views": "src/views",
    "@test": "test"
  },
  "scripts": {
    "dev": "NODE_ENV=development node dev-server.mjs",
    "compile": "NODE_ENV=production node esbuild.config.mjs",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "lint-sass": "stylelint 'src/assets/sass/**/*.scss'",
    "test": "rm -rf ./pacts && NODE_ENV=test mocha '!(node_modules)/**/*.test'.js",
    "test:no-pact": "rm -rf ./pacts && NODE_ENV=test mocha --exclude **/*.pact.test.js '!(node_modules)/**/*.test'.js",
    "test:pact": "rm -rf ./pacts && NODE_ENV=test mocha **/*.pact.test.js",
    "cypress:server": "run-amock --port=8000 | node -r dotenv/config dist/application.js dotenv_config_path=test/cypress/test.env",
    "cypress:dev-server": "run-amock --port=8000 | NODE_ENV=test node dev-server.mjs",
    "cypress:test": "cypress run",
    "cypress:test-headed": "cypress open",
    "publish-pacts": "./bin/publish-pacts.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "overrides": {
    "minimist": "1.2.6",
    "qrcode": {
      "ansi-regex": "3.0.1"
    },
    "cli-color": {
      "ansi-regex": "3.0.1"
    },
    "csurf": {
      "cookie": "0.7.2"
    },
    "@sentry/node": {
      "cookie": "0.7.2"
    },
    "staticify": {
      "send": "0.19.0"
    },
    "cross-spawn": "^7.0.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.693.0",
    "@aws-sdk/s3-request-presigner": "3.693.0",
    "@govuk-pay/pay-js-commons": "^7.0.2",
    "@govuk-pay/pay-js-metrics": "^1.0.14",
    "@sentry/node": "6.12.0",
    "accessible-autocomplete": "2.0.4",
    "axios": "^1.8.2",
    "body-parser": "1.20.3",
    "change-case": "3.1.0",
    "check-types": "11.2.x",
    "chokidar": "^3.5.3",
    "client-sessions": "0.8.0",
    "common-password": "0.1.2",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.x",
    "express": "4.21.2",
    "express-validator": "^7.2.0",
    "google-libphonenumber": "3.2.33",
    "govuk-frontend": "^5.7.1",
    "http-proxy": "1.18.x",
    "https-proxy-agent": "5.0.1",
    "joi": "17.12.1",
    "lodash": "4.17.21",
    "luhn-js": "^1.1.2",
    "luxon": "^3.5.0",
    "module-alias": "^2.2.3",
    "moment-timezone": "0.5.43",
    "morgan": "1.10.0",
    "multer": "^2.0.0",
    "nunjucks": "3.2.4",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-local": "^1.0.0",
    "qrcode": "1.5.3",
    "qs": "6.11.2",
    "rfc822-validate": "1.0.0",
    "staticify": "5.0.0",
    "stripe": "^10.12.0",
    "throng": "5.0.x",
    "uk-postcode": "0.1.x",
    "url-join": "4.0.1",
    "validator": "^13.11.0",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@eslint/js": "^9.23.0",
    "@govuk-pay/run-amock": "0.0.9",
    "@pact-foundation/pact": "^12.1.1",
    "@pact-foundation/pact-core": "^14.0.5",
    "@types/client-sessions": "^0.8.6",
    "@types/express": "^5.0.1",
    "@types/jquery": "^3.5.32",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.6.2",
    "@types/mocha": "^10.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.14",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.4",
    "axe-core": "^4.10.3",
    "chai": "4.5.0",
    "chai-arrays": "2.2.0",
    "chai-as-promised": "7.1.2",
    "cheerio": "1.0.0-rc.12",
    "chokidar-cli": "*",
    "csrf": "^3.1.0",
    "cypress": "^13.17.0",
    "cypress-axe": "^1.6.0",
    "dotenv": "16.3.1",
    "esbuild": "^0.25.0",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "gaap-analytics": "3.1.0",
    "globals": "^16.0.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^15.5.1",
    "mocha": "^10.2.0",
    "nock": "13.3.3",
    "notp": "2.0.3",
    "portfinder": "1.0.32",
    "prettier": "3.5.3",
    "proxyquire": "~2.1.0",
    "sass": "^1.66.1",
    "sinon": "^15.2.0",
    "sinon-chai": "3.7.0",
    "stylelint": "^15.10.3",
    "stylelint-config-gds": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "supertest": "^6.3.3",
    "ts-loader": "^9.5.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "wick-a11y": "^1.4.0"
  }
}
