{
  "name": "pay-selfservice",
  "description": "Payments Selfservice application",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^18.20.4"
  },
  "_moduleAliases": {
    "@root": "src",
    "@controllers": "src/controllers",
    "@middleware": "src/middleware",
    "@models": "src/models",
    "@services": "src/services",
    "@utils": "src/utils",
    "@views": "src/views",
    "@test": "test"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect",
      "cy",
      "Cypress"
    ],
    "ignore": [
      "src/assets/**/*.js",
      "dist/"
    ]
  },
  "scripts": {
    "dev": "NODE_ENV=development node dev-server.mjs",
    "compile": "NODE_ENV=production node esbuild.config.mjs",
    "lint": "standard --fix",
    "lint-sass": "stylelint 'src/assets/sass/**/*.scss'",
    "test": "rm -rf ./pacts && NODE_ENV=test mocha '!(node_modules)/**/*.test'.js",
    "test:no-pact": "rm -rf ./pacts && NODE_ENV=test mocha --exclude **/*.pact.test.js '!(node_modules)/**/*.test'.js",
    "test:pact": "rm -rf ./pacts && NODE_ENV=test mocha **/*.pact.test.js",
    "cypress:server": "run-amock --port=8000 | node -r dotenv/config dist/application.js dotenv_config_path=test/cypress/test.env",
    "cypress:dev-server": "run-amock --port=8000 | NODE_ENV=test node dev-server.mjs",
    "cypress:test": "cypress run",
    "cypress:test-headed": "cypress open",
    "publish-pacts": "./bin/publish-pacts.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "overrides": {
    "minimist": "1.2.6",
    "qrcode": {
      "ansi-regex": "3.0.1"
    },
    "cli-color": {
      "ansi-regex": "3.0.1"
    },
    "csurf": {
      "cookie": "0.7.2"
    },
    "@sentry/node": {
      "cookie": "0.7.2"
    },
    "staticify": {
      "send": "0.19.0"
    },
    "cross-spawn": "^7.0.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.693.0",
    "@aws-sdk/s3-request-presigner": "3.693.0",
    "@govuk-pay/pay-js-commons": "^6.0.22",
    "@govuk-pay/pay-js-metrics": "^1.0.6",
    "@sentry/node": "6.12.0",
    "accessible-autocomplete": "2.0.4",
    "axios": "^1.7.4",
    "body-parser": "1.20.3",
    "change-case": "3.1.0",
    "check-types": "11.2.x",
    "chokidar": "^3.5.3",
    "client-sessions": "0.8.0",
    "common-password": "0.1.2",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.x",
    "express": "4.21.2",
    "express-validator": "^7.2.0",
    "google-libphonenumber": "3.2.33",
    "govuk-frontend": "^5.7.1",
    "http-proxy": "1.18.x",
    "https-proxy-agent": "5.0.1",
    "joi": "17.12.1",
    "lodash": "4.17.21",
    "luhn-js": "^1.1.2",
    "luxon": "^3.5.0",
    "module-alias": "^2.2.3",
    "moment-timezone": "0.5.43",
    "morgan": "1.10.0",
    "multer": "^1.4.5-lts.1",
    "nunjucks": "3.2.4",
    "passport": "0.5.x",
    "passport-custom": "1.1.1",
    "passport-local": "1.0.0",
    "qrcode": "1.5.3",
    "qs": "6.11.2",
    "rfc822-validate": "1.0.0",
    "staticify": "5.0.0",
    "stripe": "^10.12.0",
    "throng": "5.0.x",
    "uk-postcode": "0.1.x",
    "url-join": "4.0.1",
    "validator": "^13.11.0",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@govuk-pay/run-amock": "0.0.7",
    "@pact-foundation/pact": "^12.1.1",
    "@pact-foundation/pact-core": "^14.0.5",
    "@types/jquery": "^3.5.32",
    "chai": "4.5.0",
    "chai-arrays": "2.2.0",
    "chai-as-promised": "7.1.2",
    "cheerio": "1.0.0-rc.12",
    "chokidar-cli": "*",
    "csrf": "^3.1.0",
    "cypress": "^13.8.0",
    "dotenv": "16.3.1",
    "esbuild": "^0.24.2",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-sass-plugin": "^3.3.1",
    "gaap-analytics": "3.1.0",
    "jsdom": "^21.1.1",
    "lint-staged": "14.0.1",
    "mocha": "^10.2.0",
    "nock": "13.3.3",
    "notp": "2.0.3",
    "portfinder": "1.0.32",
    "proxyquire": "~2.1.0",
    "sass": "^1.66.1",
    "sinon": "15.2.0",
    "sinon-chai": "3.7.0",
    "standard": "^17.1.2",
    "stylelint": "^15.10.3",
    "stylelint-config-gds": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "supertest": "^6.3.3"
  }
}
